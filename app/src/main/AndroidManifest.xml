<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pennappspring15.bullethole.bullethole" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.pennappspring15.bullethole.bullethole.FirstActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.pennappspring15.bullethole.bullethole.MainActivity"
            android:label="@string/app_name"
            android:parentActivityName="com.pennappspring15.bullethole.bullethole.FirstActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.pennappspring15.bullethole.bullethole.FirstActivity"/>
        </activity>
        <activity
            android:name="com.pennappspring15.bullethole.bullethole.EndActivity"
            android:label="@string/app_name"
            android:parentActivityName="com.pennappspring15.bullethole.bullethole.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.pennappspring15.bullethole.bullethole.MainActivity"/>
        </activity>
    </application>

</manifest>
